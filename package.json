{
  "name": "nti",
  "version": "0.0.1",
  "description": "Npm Task Info: Interactive cli menu shown description to list and run npm tasks",
  "repository": "sculove/nti",
  "author": {
    "name": "sculove",
    "url": "http://sculove.github.io"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "pretest": "xo",
    "test": "ava # this is description"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "npm",
    "task",
    "list",
    "interactive",
    "inquirer",
    "cli-app",
    "cli",
    "nti",
    "ntl",
    "description",
    "runner",
    "menu"
  ],
  "dependencies": {
    "inquirer": "^0.12.0",
    "minimist": "^1.2.0",
    "pkginfo": "~0.3.1"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "tempdir": "^1.0.0",
    "tempfile": "^1.1.1",
    "xo": "^0.12.1"
  },
  "xo": {
    "rules": {
      "consistent-return": 0
    }
  },
  "ava": {
    "files": [
      "test/index.js"
    ],
    "tap": true,
    "verbose": true
  },
  "license": "MIT"
}
